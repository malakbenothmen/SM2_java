package TD4;

public class Livre {
	protected String title,domaine;
	protected int id,nbPage;
	protected final double TVA=0.1;
	
	
	public String toString()
	{return "c'est un "+this.getClass().getSimpleName()+" titre: "+this.title+" id: "+this.id+" Domaine"+this.domaine+" nombre de pages: "+this.nbPage;}
	
	

}
