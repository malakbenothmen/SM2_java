package td3_ex2;

public class main {

	public static void main(String[] args) {
		
		Dictionnaire d=new Dictionnaire("Larousse",5);
		d.ajouterMot("abondonner");
		d.ajouterMot("abondance");
		d.ajouterMot("");
		d.listerDico();
		System.out.println(d.chercherMot(null));
		System.out.println(d.chercherMot(null));
		System.out.println("le nombre de synonymes de  est"+d.nbSynonymes(null));
		d.supprimerMot(null);
		d.listerDico();
		
		

	}

}
